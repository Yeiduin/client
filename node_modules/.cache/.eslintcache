reventDefault();\r\n\r\n    dispatch(postTouristActivy(input));\r\n    dispatch(getAllCountries());\r\n    alert(\"has creado una nueva actividad\");\r\n  }\r\n  function limpiar() {\r\n    let form = document.getElementById(\"45\");\r\n    form.reset();\r\n    setInput({\r\n      name: \"\",\r\n      dificult: \"\",\r\n      duration: \"\",\r\n      season: \"Verano\",\r\n      pais: [],\r\n    });\r\n  }\r\n\r\n  function validacionInput(e) {\r\n    if (expresiones.usuario.test(input.name)) {\r\n      console.log(\"nombre OK\");\r\n    } else {\r\n      e.target.value = \"\";\r\n      setInput({ ...input, name: \"\" });\r\n      alert(\r\n        \"el nombre debe tener entre 4 y 35 caracteres y no debe tener caracteres especiales\"\r\n      );\r\n    }\r\n  }\r\n\r\n  return (\r\n    <div className={style.divgeneral}>\r\n      <Link to=\"/Home\">\r\n        <button>Volver</button>\r\n      </Link>\r\n      <h1>Crear una actividad turistica</h1>\r\n      <div>\r\n        <form\r\n          id=\"45\"\r\n          onSubmit={(e) => {\r\n            postear(e);\r\n            limpiar();\r\n          }}\r\n        >\r\n          <label>\r\n            <p>Nombre</p>\r\n          </label>\r\n          <input\r\n            className={style.inputtext}\r\n            type=\"text\"\r\n            required\r\n            value={input.name}\r\n            name=\"name\"\r\n            id=\"nombree\"\r\n            onChange={tomarValores}\r\n            onBlur={(e) => validacionInput(e)}\r\n          />\r\n          <label>\r\n            <p>Dificultad</p>\r\n          </label>\r\n          <input\r\n            type=\"number\"\r\n            className={style.inputnum}\r\n            required={true}\r\n            min={1}\r\n            max={5}\r\n            value={input.dificult}\r\n            name=\"dificult\"\r\n            onChange={tomarValores}\r\n          />\r\n          <label>\r\n            <p>Duracion en dias</p>\r\n          </label>\r\n          <input\r\n            className={style.inputnum}\r\n            type=\"number\"\r\n            required\r\n            min={1}\r\n            max={365}\r\n            value={input.duration}\r\n            name=\"duration\"\r\n            onChange={tomarValores}\r\n          />\r\n          <label>\r\n            <p>Temporada</p>\r\n          </label>\r\n          <select name=\"season\" required onChange={tomarValores}>\r\n            {temporadas.map((e, o) => (\r\n              <option key={o} value={e}>\r\n                {e}\r\n              </option>\r\n            ))}\r\n          </select>\r\n          <label>\r\n            <p>Paises</p>\r\n          </label>\r\n          <select name=\"pais\" required onChange={tomarSelec}>\r\n            {paisesParaSelec.map((e) => (\r\n              <option key={e.id} value={e.name}>\r\n                {e.name}\r\n              </option>\r\n            ))}\r\n          </select>\r\n          <label>\r\n            <p>gracias</p>\r\n            <button type=\"submit\" onClick={(e) => setSave(save + 1)}>\r\n              enviar\r\n            </button>\r\n            <button onClick={limpiar}>limpiar formulario</button>\r\n          </label>\r\n          <ul>\r\n            {input.pais.map((el, i) => (\r\n              <li key={i}>{el + \" - \"}</li>\r\n            ))}\r\n          </ul>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Crearactividadturist;\r\n","C:\\Users\\User\\Desktop\\proyectos\\cosas de herny programacion\\proyecto individual\\PI-Countries-main\\client\\src\\components\\funciones.js",[],"C:\\Users\\User\\Desktop\\deploy\\client\\src\\index.js",[],"C:\\Users\\User\\Desktop\\deploy\\client\\src\\App.js",[],"C:\\Users\\User\\Desktop\\deploy\\client\\src\\reportWebVitals.js",[],"C:\\Users\\User\\Desktop\\deploy\\client\\src\\redux\\store\\index.js",[],"C:\\Users\\User\\Desktop\\deploy\\client\\src\\components\\DetallesDePais.js",[],"C:\\Users\\User\\Desktop\\deploy\\client\\src\\component